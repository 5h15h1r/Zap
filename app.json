{
  "expo": {
    "name": "DelieveryApp",
    "slug": "DelieveryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-color.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-no-background.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFACD"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-color.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidStatusBar": {
      "translucent": true // Ensure translucency for fullscreen
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
